  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug",
      "protocol": "auto",
      "port": 1369,
      "sourceMaps": true,
      "restart": true,
      "localRoot": "${workspaceFolder}",
      "outFiles": ["${workspaceFolder}/lib/**/*.js"],
      "remoteRoot": "/",
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
